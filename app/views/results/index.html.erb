<h1>Tweets about</h1>
<div id="map"></div>
<script type='text/javascript'>
      function initMap() {
        var uluru = {lat: -25.363, lng: 131.044};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 4,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
<script async defer
src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['google_location_api'] %>&callback=initMap">
</script>
